{
  "name": "@sigilnet/fft-legacy",
  "version": "0.0.10",
  "description": "Simple pure Javascript implementation of the Cooley-Tukey algorithm. Note: fft-js was chosen as the name since a lot of the FFT implementations on NPM at the time this was published were wrappers for Ruby or C.",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./fft": {
      "types": "./dist/types/fft.d.ts",
      "import": "./dist/fft.js",
      "require": "./dist/fft.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "index.js",
    "fft.js",
    "src",
    "test"
  ],
  "keywords": [
    "fft",
    "dft",
    "fast",
    "discrete",
    "fourier",
    "transform",
    "audio",
    "speech",
    "cooley",
    "tukey"
  ],
  "scripts": {
    "build:tsc": "tsc -p tsconfig.json",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "build": "pnpm run clean && vite build && pnpm run build:tsc && pnpm run build:types && pnpm run declaration && pnpm run build:umd",
    "build:umd": "rollup -c",
    "declaration": "tsc -p tsconfig.declaration.json",
    "build:types": "tsc -p tsconfig.json --emitDeclarationOnly --declarationMap --outDir dist/types",
    "dev": "vite",
    "test": "jest",
    "test:mocha": "mocha"
  },
  "dependencies": {
    "bit-twiddle": "~1.0.2",
    "commander": "~14.0.2",
    "@octokit/request-error": "^7.0.1",
    "dev": "^0.1.3",
    "pino": "^10.1.0",
    "snowflake-sdk": "^2.3.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-typescript": "12.3.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@types/mocha": "^10.0.10",
    "eslint": "^9.39.1",
    "jest": "^29.5.0",
    "mocha": "^10.2.0",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.1",
    "rollup": "^4.53.3",
    "vite": "^7.2.4",
    "typescript": "5.9.3"
  }
}
